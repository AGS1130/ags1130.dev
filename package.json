{
  "name": "ags1130.dev",
  "version": "1.0.0",
  "description": "Personal dev website of Gregory Salinas",
  "author": "Gregory Salinas",
  "scripts": {
    "start": "nuxt",
    "serve": "nuxt start",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "format": "yarn format:js && yarn format:css",
    "format:js": "prettier --write \"**/*.{js,vue}\" --ignore-path .gitignore .",
    "format:css": "prettier --write \"**/*.css\" --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:css": "stylelint **/*.{vue,css} --ignore-path .gitignore"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^6.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^4.0.1",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "cssnano-preset-advanced": "^4.0.7",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nuxt-vite": "^0.0.36",
    "postcss": "^8.2.8",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "tailwind-config-viewer": "^1.5.0",
    "tailwindcss": "^2.0.3",
    "theme-colors": "^0.0.5"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "nuxt": "^2.15.3",
    "prism-themes": "^1.6.0"
  }
}
